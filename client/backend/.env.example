# Transition DJ Backend 환경 설정

# ===== 서버 설정 =====
HOST=0.0.0.0
PORT=8000
DEBUG=true

# ===== 파일 저장 경로 =====
UPLOAD_DIR=./uploads
OUTPUT_DIR=./outputs

# ===== Demucs 설정 =====
# 사용할 모델: htdemucs, htdemucs_ft, htdemucs_6s
DEMUCS_MODEL=htdemucs
# GPU 사용 여부 (cuda, cpu)
DEMUCS_DEVICE=cpu

# ===== CORS 허용 Origin =====
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# ===== 워커 설정 =====
# 동시 처리 작업 수
MAX_WORKERS=2
# 작업 타임아웃 (초)
JOB_TIMEOUT=600
